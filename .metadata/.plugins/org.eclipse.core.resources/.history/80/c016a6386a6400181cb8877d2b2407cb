package algo_project;
import javax.swing.*;

public class Main {
	
	public static void main(String[] args){
		JFrame frame = new JFrame("Welcome");
		frame.setResizable(123);//是否允许用户重新定义窗口大小
		frame.setSize(500, 500);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);//设置X的关闭功能
		frame.setVisible(true);
	}
}
